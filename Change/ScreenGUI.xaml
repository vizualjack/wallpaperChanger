<UserControl x:Class="WallpaperChanger.Change.ScreenGUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WallpaperChanger.Change"
             mc:Ignorable="d" Margin="5">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto" MinHeight="39.96"/>
        </Grid.RowDefinitions>
        <Image x:Name="image" Grid.Row="0"/>
        <Button x:Name="changeBtn" Grid.Row="1" Content="Change" HorizontalAlignment="Center" Width="100" Height="19.96" Click="changeBtn_Click" Margin="0,10,0,10"/>
    </Grid>
</UserControl>
